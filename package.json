{
  "name": "my-next-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "wagmi generate && npx prettier --write src/generated.ts && next build",
    "start": "next start",
    "lint": "next lint",
    "pages:build": "npx @cloudflare/next-on-pages",
    "preview": "wrangler pages dev .vercel/output/static",
    "deploy": "wrangler pages deploy .vercel/output/static",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv env.d.ts",
    "prebuild": "node scripts/generate-robots-txt.js",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx}\" && eslint --fix \"**/*.{js,jsx,ts,tsx}\"",
    "lint:json": "eslint '**/*.{json,json5,jsonc}'",
    "openapi-ts": "openapi-ts",
    "wagmi:generate": "wagmi generate"
  },
